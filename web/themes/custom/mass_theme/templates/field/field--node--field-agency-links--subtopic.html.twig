{% set links = {} %}

{# Use our array to wrap up our links #}
{% for item in items %}
  {% set links = links|merge({ (loop.index0) : {'url': item.content['#url'].getUri(), 'text': item.content['#title']} }) %}
{% endfor %}

{%
set linkList = {
'title': label,
'links': links
}
%}

