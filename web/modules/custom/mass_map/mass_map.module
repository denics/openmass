<?php

/**
 * @file
 * Contains mass_map.module.
 */

/**
 * Implements hook_theme().
 */
function mass_map_theme() {
  $theme['map_page'] = [
    'variables' => ['map' => NULL],
    'template' => 'map-page',
  ];
  $theme['map_row'] = [
    'variables' => ['map' => NULL],
    'template' => 'map-row',
  ];
  return $theme;
}
