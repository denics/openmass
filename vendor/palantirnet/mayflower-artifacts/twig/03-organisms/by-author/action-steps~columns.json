{
  "actionSteps": {
    "intro": "Follow these tips to ease your move on the big day:",
    "columns": [{
      "heading":"Choose a Reputable Mover",
        "steps": [{
        "icon":"@atoms/05-icons/svg-circle-chevron.twig",
        "title":"",
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph": {
              "text": "The <a href=\"#\">Executive Office of Energy and Environmental Affairs (EEA)</a> recommends <a href=\"#\">selecting a reputable mover</a>."
            }
          }
        }]
      },{
        "icon":"@atoms/05-icons/svg-circle-chevron.twig",
        "title":"",
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph": {
              "text": "Make sure that any in-state moving company you use is properly licensed with the <a href=\"#\" target=\"_blank\">Massachusetts Department of Public Utilities (DPU)</a>."
            }
          }
        }]
      },{
        "icon":"@atoms/05-icons/svg-circle-chevron.twig",
        "title":"",
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph": {
              "text": "Follow the <a href=\"#\">Federal Motor Carrier Safety Administration (FMCSA)</a> guidelines to protect yourself from moving fraud."
            }
          }
        }]
      }]
    },{
      "heading":"Observe Parking Restrictions",
        "steps": [{
        "icon":"@atoms/05-icons/svg-circle-chevron.twig",
        "title":"",
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph": {
              "text": "Research <a href=\"#\">parking restrictions</a> before moving to find out where you are allowed to unload vehicles."
            }
          }
        }]
      },{
        "icon":"@atoms/05-icons/svg-circle-chevron.twig",
        "title":"",
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph": {
              "text": "<a href=\"#\" target=\"_blank\">Contact your city or town</a> for information on obtaining a moving permit, or talk to your landlord or property owner."
            }
          }
        }]
      }]
    }]
  }
}