{
    "name": "palantirnet/mass",
    "description": "A Drupal 8 project for the Commonwealth of Massachusetts.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Palantir.net",
            "email": "info@palantir.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/mayflower-artifacts"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/the-build.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/the-vagrant.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/workbench_tabs.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:mikeyp/google_tag.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/palantir-behat-extension.git"
        },
        {
            "type": "pear",
            "url": "https://pear.php.net"
        },
        {
            "type": "package",
            "package": {
                "name": "desandro/imagesloaded",
                "version": "v4.1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/imagesloaded/archive/v4.1.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "4.1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v4.1.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.3.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "previousnext/drush_cmi_tools",
                "version": "1.0",
                "dist": {
                    "url": "https://github.com/previousnext/drush_cmi_tools/archive/8.x-1.x.zip",
                    "type": "zip"
                }
            }
        },
      {
        "type": "package",
        "package": {
          "name": "blakestein/datalayer",
          "version": "master",
          "type": "drupal-module",
          "source": {
            "url": "https://github.com/blakestein/drupal-datalayer-d8.git",
            "type": "git",
            "reference": "3b2ffa7"
          }
        }
      }
    ],
    "require": {
        "acquia/acquia-sdk-php": "0.10.8",
        "blakestein/datalayer": "master",
        "christian-riesen/base32": "^1.3.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "~1.0",
        "desandro/imagesloaded": "^4.1",
        "desandro/masonry": "^4.1",
        "drupal-composer/drupal-scaffold": "^1.0",
        "drupal/access_unpublished": "1.x-dev",
        "drupal/acquia_connector": "^1",
        "drupal/acquia_purge": "^1",
        "drupal/address": "~1.0",
        "drupal/better_field_descriptions": "1.x-dev",
        "drupal/clamav": "^1.0",
        "drupal/components": "^1",
        "drupal/config_installer": "^1",
        "drupal/config_log": "^1",
        "drupal/console": "~1.0",
        "drupal/core": "8.2.8",
        "drupal/crazyegg": "^1",
        "drupal/diff": "1.x-dev",
        "drupal/draggableviews": "1.x-dev",
        "drupal/encrypt": "3.x-dev#19cda8cf22bfe7d020b819a7cfddf2e4883b78a8",
        "drupal/entity_browser": "^1",
        "drupal/entity_reference_revisions": "~1.0",
        "drupal/field_group": "^1.0@RC",
        "drupal/field_tokens": "^1.0@beta",
        "drupal/file_browser": "1.x-dev",
        "drupal/flood_unblock": "^1",
        "drupal/ga_login": "^1",
        "drupal/geocoder": "^2.0@alpha",
        "drupal/geofield": "^1.0@alpha",
        "drupal/google_map_field": "^1",
        "drupal/imagick": "^1",
        "drupal/inline_entity_form": "1.x-dev",
        "drupal/libraries": "^3",
        "drupal/media_entity": "^1",
        "drupal/media_entity_document": "^1",
        "drupal/metatag": "^1",
        "drupal/migrate_plus": "^3",
        "drupal/migrate_source_csv": "^2",
        "drupal/migrate_tools": "^3",
        "drupal/node_edit_protection": "1.x-dev",
        "drupal/office_hours": "^1.0@alpha",
        "drupal/paragraphs": "1.x-dev",
        "drupal/password_policy": "3.x-dev",
        "drupal/pathauto": "^1",
        "drupal/rabbit_hole": "1.x-dev",
        "drupal/redirect": "^1",
        "drupal/restrict_by_ip": "4.x-dev",
        "drupal/restui": "^1",
        "drupal/seckit": "1.0-alpha2",
        "drupal/security_review": "1.x-dev",
        "drupal/twig_field_value": "^1",
        "drupal/twig_tweak": "^1",
        "drupal/username_enumeration_prevention": "1.0-beta1",
        "drupal/video_embed_field": "^1.4",
        "drupal/workbench_moderation": "^1",
        "drupal/xmlsitemap": "1.x-dev",
        "enyo/dropzone": "4.3.0",
        "mikeyp/google_tag": "dev-8.x-1.x#3caa4f0b7f008576936ab7501d483bfcf58a4764",
        "palantirnet/mayflower-artifacts": "5.0.0",
        "palantirnet/the-build": "^1.3.3",
        "palantirnet/the-vagrant": "dev-master",
        "palantirnet/workbench_tabs": "dev-8.x-1.x",
        "previousnext/drush_cmi_tools": "^1"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "composer/composer": "@dev",
        "dflydev/embedded-composer-console": "@dev",
        "dflydev/embedded-composer-core": "@dev",
        "drupal/devel": "1.x-dev",
        "drupal/drupal-extension": "^3.1",
        "drush/drush": "8.1.10",
        "palantirnet/palantir-behat-extension": "dev-drupal8",
        "squizlabs/php_codesniffer": "2.*",
        "drupal/coder": "^8.2.12"
    },
    "suggest": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "scripts": {
        "post-install-cmd": "@remove-submodules",
        "post-update-cmd": "@remove-submodules",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "remove-submodules": "rm -rf vendor/**/.git && rm -rf docroot/**/.git"
    },
    "extra": {
        "drupal-scaffold": {
            "excludes": [
                ".htaccess",
                "robots.txt"
            ],
            "omit-defaults": false
        },
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Remove drupal.js error. (https://www.drupal.org/node/2661880)": "https://www.drupal.org/files/issues/quickedit-undefined-attr-2661880-14.patch",
                "Don't break if telephone number is less than 6 digits. (https://www.drupal.org/node/2575577#comment-11750724)": "https://www.drupal.org/files/issues/2575577-11.patch",
                "Replace (in ajax_view.js) $.size with .length since .size is deprecated.": "https://www.drupal.org/files/issues/drupal-8.3.x-remove-jquery-size-for-ajax_view-2828140-3_0.patch",
                "Fix photo disappearing from org page. Fix Contextual Links js bug.": "https://www.drupal.org/files/issues/2551373-32.patch"
            },
            "drupal/workbench_moderation": {
                "Fix type errors from WorkbenchModeration edit tab labeling (https://www.drupal.org/node/2769119#comment-11624207)": "https://www.drupal.org/files/issues/workbench_moderation-edit_tab-2769119-6.patch"            },
            "drupal/crazyegg": {
              "Configure crazyegg for specific pages. (https://www.drupal.org/node/2812783)": "https://www.drupal.org/files/issues/2812783-configure-for-specific-pages.patch"
            },
            "drupal/password_policy": {
              "Removes field updates from the install file. (https://www.drupal.org/node/2771129#comment-11954803)": "https://www.drupal.org/files/issues/password-policy-config-import-field-error-2771129-10-D8.patch"
            },
            "drupal/tfa": {
                "Drush sanitize plugin for TFA user data. (https://www.drupal.org/node/2862312)": "https://www.drupal.org/files/issues/drush_27.patch"
            },
            "drupal/username_enumeration_prevention": {
                "Fix install error": "https://www.drupal.org/files/issues/username_enumeration_prevention_requirements_severity-2823440-1.patch"
            },
            "drupal/entity_browser": {
                "Fix duplicated image selection when click use selected button": "https://www.drupal.org/files/issues/prevent-duplication.patch",
                "Fix for 'Undefined index: max_resolution' which occured what looking for max_resolution on non-images (https://www.drupal.org/node/2871372)": "https://www.drupal.org/files/issues/entity-browser-widget-context-small-fix-2871372-2.patch"
            },
            "drupal/access_unpublished": {
              "Allow to view unpublished content moderated using Workbench Moderation": "https://www.drupal.org/files/issues/access_unpublished-integration_with_workbench_moderation_module-2848938-4.patch"
            },
            "drupal/field_group": {
              "Provide page level error messaging when publishing validation is not met": "https://www.drupal.org/files/issues/field_group-validation-2787179-9.patch"
            },
            "drupal/better_field_descriptions": {
                "Fix the positioning of the additional help text (https://www.drupal.org/node/2882765)": "https://www.drupal.org/files/issues/better_field_descriptions-position-help-2882765-0.patch"
            }
        }
    }
}
