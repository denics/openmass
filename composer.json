{
    "name": "palantirnet/mass",
    "description": "A Drupal 8 project for the Commonwealth of Massachusetts.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Palantir.net",
            "email": "info@palantir.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "preferred-install": "dist",
    "config": {
        "sort-packages": "true"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/mayflower-artifacts"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/the-build.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/the-vagrant.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/workbench_tabs.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:mikeyp/google_tag.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        },
        {
            "type": "vcs",
            "url": "git@github.com:palantirnet/palantir-behat-extension.git"
        },
        {
            "type": "package",
            "package": {
                "name": "previousnext/drush_cmi_tools",
                "version": "1.0",
                "dist": {
                    "url": "https://github.com/previousnext/drush_cmi_tools/archive/8.x-1.x.zip",
                    "type": "zip"
                }
            }
        },
        {
          "type": "package",
          "package": {
            "name": "enyo/dropzone",
            "version": "4.3.0",
            "dist": {
              "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
              "type": "zip"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "desandro/imagesloaded",
            "version": "dev-dc089d",
            "source": {
              "url": "https://github.com/desandro/imagesloaded.git",
              "type": "git",
              "reference": "dc089d"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "desandro/masonry",
            "version": "4.1.1",
            "dist": {
              "url": "https://github.com/desandro/masonry/archive/v4.1.1.zip",
              "type": "zip"
            }
          }
        }
    ],
    "require": {
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "~1.0",
        "desandro/imagesloaded": "dev-master",
        "desandro/masonry": "^4.1",
        "drupal-composer/drupal-scaffold": "^1.0",
        "drupal/acquia_connector": "^8.1",
        "drupal/config_installer": "^8.1",
        "drupal/core": "^8.0",
        "drupal/crazyegg": "8.*",
        "drupal/draggableviews": "8.1.x-dev",
        "drupal/entity_browser": "8.1.x-dev",
        "drupal/entity_reference_revisions": "~8.1.0",
        "drupal/field_group": "8.*",
        "drupal/file_browser": "8.1.x-dev",
        "drupal/google_map_field": "8.*",
        "drupal/imagick": "8.*",
        "drupal/inline_entity_form": "8.*",
        "drupal/metatag": "^8.1",
        "drupal/migrate_plus": "~8.2",
        "drupal/migrate_source_csv": "~8.1",
        "drupal/migrate_tools": "~8",
        "drupal/paragraphs": "~8.1.0",
        "drupal/pathauto": "^8.1",
        "drupal/redirect": "8.*",
        "drupal/seckit": "8.1.0-alpha2",
        "drupal/twig_field_value": "^8.1",
        "drupal/twig_tweak": "^8.1",
        "drupal/workbench_moderation": "^8.1",
        "enyo/dropzone": "^4.3",
        "mikeyp/google_tag": "dev-8.x-1.x#4c35b508650f3268012abd4afbe4e309eb5297da",
        "palantirnet/mayflower-artifacts": "0.5.",
        "palantirnet/the-build": "dev-feature/code-quality-tools",
        "palantirnet/the-vagrant": "dev-master",
        "palantirnet/workbench_tabs": "dev-8.x-1.x",
        "previousnext/drush_cmi_tools": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "composer/composer": "@dev",
        "dflydev/embedded-composer-console": "@dev",
        "dflydev/embedded-composer-core": "@dev",
        "drupal/console": "^0.11.3",
        "drupal/devel": "dev-8.x-1.x",
        "drupal/drupal-extension": "^3.1",
        "drush/drush": "^8.0",
        "palantirnet/palantir-behat-extension": "dev-drupal8"
    },
    "suggest": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "drupal-scaffold": {
            "excludes": [
                ".htaccess",
                "robots.txt"
            ],
            "omit-defaults": false
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Remove drupal.js error. (https://www.drupal.org/node/2661880)": "https://www.drupal.org/files/issues/quickedit-undefined-attr-2661880-14.patch"
            },
            "drupal/metatag": {
                "Stop metatag from breaking drush": "https://www.drupal.org/files/issues/metatag-deactivate_drupalconsole_services.patch"
            },
            "drupal/workbench_moderation": {
                "Fix type errors from WorkbenchModeration edit tab labeling (https://www.drupal.org/node/2769119#comment-11624207)": "https://www.drupal.org/files/issues/workbench_moderation-edit_tab-2769119-6.patch"
            },
            "drupal/draggableviews": {
                "Invalidate cache when draggableviews order is changed (https://www.drupal.org/node/2425249#comment-10716828)": "https://www.drupal.org/files/issues/draggableviews-clearcache-2425249-11-8.x-1.x.diff"
            },
            "drupal/google_map_field": {
              "Creating a setting for the google api key. (https://www.drupal.org/node/2759953)": "https://www.drupal.org/files/issues/setting-for-google-api-key-2759953.patch"
            },
            "drupal/core": {
              "Don't break if telephone number is less than 6 digits. (https://www.drupal.org/node/2575577#comment-11750724)": "https://www.drupal.org/files/issues/2575577-11.patch"
            },
            "drupal/crazyegg": {
              "Configure crazyegg for specific pages. (https://www.drupal.org/node/2812783)": "https://www.drupal.org/files/issues/2812783-configure-for-specific-pages.patch"
            }
        }
    }
}
