{#
/**
 * @file
 * Theme override to display a single node page.
 *
 * This file removes the output of the
 * pre_content and post_content regions, otherwise rendered
 * at the page level.
 *
 * @see template_preprocess_node()
 * @see page.html.twig
 *
 */
#}
{% extends 'page.html.twig' %}
{% block massFlaggingLinkContainer %}
  {% if logged_in %}
    <div class="mass-flagging-link-container">
      {{ page.watch_content_link }}
      {{ page.flag_content_link }}
    </div>
  {% endif %}
{% endblock %}
{% block preContent %}{% endblock %}
{% block postContent %}{% endblock %}
