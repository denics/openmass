  RewriteCond %{HTTP_HOST} !^edit(\.stage|\.pilot)?\.mass\.[a-z]+ [NC]
  RewriteCond %{ENV:AH_SITE_ENVIRONMENT} ^(test|prod)$
  RewriteRule ^((admin|scripts|profile|includes|cron\.php|install\.php|update\.php|xmlrpc\.php)/?(.*)?)$ - [F]

